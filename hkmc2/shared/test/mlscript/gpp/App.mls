:global
:p

f(x)(y)(z)
//│ |f|(|x|)|(|y|)|(|z|)|
//│ Pretty-print: ((f(x))(y))(z)
//│ AST: App(App(App(Var(f),Var(x)),Var(y)),Var(z))

1 + f(x) * 3
//│ |1| |+| |f|(|x|)| |*| |3|
//│ Pretty-print: 1 + f(x) * 3
//│ AST: App(App(Var(+),Const(IntLit(1))),App(App(Var(*),App(Var(f),Var(x))),Const(IntLit(3))))

f(x) + f(y)(z)
//│ |f|(|x|)| |+| |f|(|y|)|(|z|)|
//│ Pretty-print: f(x) + (f(y))(z)
//│ AST: App(App(Var(+),App(Var(f),Var(x))),App(App(Var(f),Var(y)),Var(z)))
